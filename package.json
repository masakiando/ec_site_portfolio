    "style-loader": "0.18.2",
    "webpack": "2.6.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-dev-server": "2.7.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "express": "4.16.2",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "rimraf": "^2.6.2"
