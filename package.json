    "babel-preset-stage-2": "6.24.1",
    "babel-register": "^6.26.0",
    "chai": "3.5.0",
    "chai-http": "2.0.1",
    "colors": "^1.3.0",
    "css-loader": "0.28.4",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "expect": "1.19.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "2.30.1",
